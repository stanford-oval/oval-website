<style>
	main {
		color: white;
		font-size: 20px;
		line-height: 1.6;
	}
	h4 {
		color: rgb(var(--accent));
	}
	.link-card {
		background-color: #F6F0E4;
	}
	.footer a {
		color: rgb(var(--accent-bright));
		font-weight: 600;
	}
	
	
	.header {
		
		font-weight: 700;
		line-height: 1;
		text-align: center;
		margin-bottom: 0;
		color: rgb(var(--accent));

	}
	li, p {
		color: rgb(var(--accent))
	}

	
	.link-card-grid {
		display: grid;
		grid-template-columns: repeat(auto-fit, minmax(24ch, 1fr));
		gap: 2rem;
		padding: 0;
	}
	
</style>
