{
	"version": 3,
	"routes": [
		{
			"src": "/releases/(?:\\/(.*?))?",
			"headers": {
				"Location": "/https:/crowdie.stanford.edu/[...path]"
			},
			"status": 301
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/releases(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		}
	]
}